{
    "name": "mhary/cron-bundle",
    "description": "With the CronBundle you can configure cron jobs in configuration files",
    "keywords": ["cron", "cronjob", "background", "tasks", "crontab", "symfony3", "bundle"],
    "license": "MIT",
    "type": "symfony-bundle",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Sebastian Kueck",
            "email": "seb.kueck@gmail.com"
        },
        {
            "name": "Michał Harężlak"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/framework-bundle": "2.8.*|^3.0",
        "mtdowling/cron-expression": "1.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3",
        "symfony/options-resolver": "~2.1",
        "symfony/process": "~2.1",
        "symfony/console": "~2.1",
        "monolog/monolog": "~1.7.0"
    },
    "autoload": {
        "psr-0": { "Mhary\\Bundle\\CronBundle": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    },
    "target-dir": "Mhary/Bundle/CronBundle"
}
